              <xsl:if test="@HasErrors='true'">
                <h2>
                  <xsl:value-of select="@FileName" />
                </h2>
                <table cellpadding="2" cellspacing="0" width="98%" border="1" bordercolor="white" class="infotable">
                  <tr>
                    <td nowrap="1" class="header">Line</td>
                    <td nowrap="1" class="header">Warning Type</td>
                    <td nowrap="1" class="header">Additional Information</td>
                  </tr>
                  <xsl:if test="@UnsupportedDefaultNamespace='true'">
                    <tr class="row">
                      <td class="content">
                        <xsl:value-of select="@LineNumber" />
                      </td>
                      <td class="content">
                        <xsl:if test="@UserControl='true'">
                        Warning: The root UserControl is not in the required default namespace.
                      </xsl:if>
                        <xsl:if test="not(@UserControl='true')">
                        Warning: The root control is in an unsupported namespace: <xsl:value-of select="@Namespace" />.
                      </xsl:if>
                      </td>
                      <td class="content">
                        <xsl:if test="@UserControl='true'">
                        Native code files were not generated for this file.  UserControls must be declared with a default namespace of http://schemas.microsoft.com/winfx/2006/xaml/presentation
                      </xsl:if>
                        <xsl:if test="not(@UserControl='true')">
                        Native code files were not generated for this file.  Use a root control that is from one of the following namespaces:
                        <ul><li>http://schemas.microsoft.com/winfx/2006/xaml/presentation</li><li>http://schemas.microsoft.com/xps/2005/06</li><li>http://schemas.microsoft.com/client/2007</li><li>http://schemas.microsoft.com/client/2007/deployment</li><li>clr-namespace:System;assembly=mscorlib</li><li>clr-namespace:System.Windows;assembly=System.Windows</li></ul></xsl:if>
                      </td>
                    </tr>
                  </xsl:if>
                  <xsl:for-each select="UnknownControl">
                    <tr class="row">
                      <td class="content">
                        <xsl:value-of select="@LineNumber" />
                      </td>
                      <td class="content">
                      Warning:  unknown controls detected.  Custom controls are not supported by this version of XAML for Windows Embedded.
                    </td>
                      <td class="content">
                      The generated project may not build.  Remove <xsl:value-of select="@Name" /> from the file or if it’s a custom control, manually register the control.
                    </td>
                    </tr>
                  </xsl:for-each>
                  <xsl:for-each select="MissingXClass">
                    <tr class="row">
                      <td class="content">
                        <xsl:value-of select="@LineNumber" />
                      </td>
                      <td class="content">
                      Error: XRPack.exe requires the root element to have either an x:Class or an xrpack:ClassResourceId attribute.
                    </td>
                      <td class="content">
                      The generated project will not run.  The root element, <xsl:value-of select="@Name" />, needs an x:Class or an xrpack:ClassResourceId attribute defined.
                    </td>
                    </tr>
                  </xsl:for-each>
                  <xsl:for-each select="UnknownElement">
                    <tr class="row">
                      <td class="content">
                        <xsl:value-of select="@LineNumber" />
                      </td>
                      <td class="content">
                      Error: UserControl missing.
                    </td>
                      <td class="content">
                      The generated project will not build.  The startup control must be a UserControl.
                    </td>
                    </tr>
                  </xsl:for-each>
                  <xsl:for-each select="MediaElement">
                    <xsl:for-each select="MediaElementDetail">
                      <tr class="row">
                        <td class="content">
                          <xsl:value-of select="@LineNumber" />
                        </td>
                        <td class="content">
                        Warning: <xsl:value-of select="@Name" />  MediaElements are not supported by this version of XAML for Windows Embedded.
                      </td>
                        <td class="content">
                        The generated project may not build or run.
                      </td>
                      </tr>
                    </xsl:for-each>
                  </xsl:for-each>
                  <xsl:for-each select="EffectElement">
                    <tr class="row">
                      <td class="content">
                        <xsl:value-of select="@LineNumber" />
                      </td>
                      <td class="content">
                      Warning: <xsl:value-of select="@Name" /> may not be supported by your platform. If your platform doesn't support the shader effect then this feature will not work on your device.
                    </td>
                      <td class="content">
                      The generated application may not run.  Verify that the platform supports the Effect.  The platform must support the Effect in order for it to work.
                    </td>
                    </tr>
                  </xsl:for-each>
                  <xsl:for-each select="BindingMarkup">
                    <xsl:for-each select="BindingMarkupDetail">
                      <tr class="row">
                        <td class="content">
                          <xsl:value-of select="@LineNumber" />
                        </td>
                        <td class="content">
                        Error: Binding Markup attribute <xsl:value-of select="@Name" /> is not supported by this version of XAML for Windows Embedded.
                      </td>
                        <td class="content">
                        The generated project will not build.  Remove the unsupported attribute from the file.
                      </td>
                      </tr>
                    </xsl:for-each>
                  </xsl:for-each>
                  <xsl:for-each select="DataContext">
                    <xsl:for-each select="DataContextDetail">
                      <tr class="row">
                        <td class="content">
                          <xsl:value-of select="@LineNumber" />
                        </td>
                        <td class="content">
                        Error: DataContext attribute <xsl:value-of select="@Name" /> is not supported by this version of XAML for Windows Embedded.
                      </td>
                        <td class="content">
                        The generated project will not build.  Remove the unsupported attribute from the file.
                      </td>
                      </tr>
                    </xsl:for-each>
                  </xsl:for-each>
                  <xsl:for-each select="DisplayMemberPath">
                    <xsl:for-each select="DisplayMemberPathDetail">
                      <tr class="row">
                        <td class="content">
                          <xsl:value-of select="@LineNumber" />
                        </td>
                        <td class="content">
                        Error: DisplayMemberPath attribute <xsl:value-of select="@Name" /> is not supported by this version of XAML for Windows Embedded.
                      </td>
                        <td class="content">
                        The generated project will not build.  Remove the unsupported attribute from the file.
                      </td>
                      </tr>
                    </xsl:for-each>
                  </xsl:for-each>
                  <xsl:for-each select="OverloadedEventHandler">
                    <tr class="row">
                      <td class="content">
                        <xsl:value-of select="@LineNumber" />
                      </td>
                      <td class="content">
                        Error: event handlers with multiple argument types detected.  Event handlers cannot have multiple argument types in this version of XAML for Windows Embedded.
                      </td>
                      <td class="content">
                        The generated project will not build.  Remove or rename the event handler: <xsl:value-of select="@Handler" />.
                      </td>
                    </tr>
                  </xsl:for-each>
                </table>
              </xsl:if>
            </xsl:for-each>
          </xsl:if>
          <h2>Element Use Statistics:</h2>
          <table cellpadding="2" cellspacing="0" width="98%" border="1" bordercolor="white" class="infotable">
            <tr>
              <td nowrap="1" class="header" align="right" style="padding-left:5px">Count</td>
              <td nowrap="1" class="header">Item Type</td>
            </tr>
            <xsl:for-each select="$project/Statistics/ProjectSummaryStatistics/ElementSummaryStatistic">
              <tr class="row">
                <td class="numbercontent" align="right">
                  <xsl:value-of select="@Count" />
                </td>
                <td>
                  <xsl:value-of select="@Name" />
                </td>
              </tr>
            </xsl:for-each>
            <tr class="row">
              <td class="numbercontent" align="right">
                <xsl:value-of select="$project/Statistics/Paths/@Count" />
              </td>
              <td>
                <xsl:text>Path</xsl:text>
              </td>
            </tr>
            <tr class="row">
              <td class="numbercontent" align="right">
                <xsl:value-of select="$project/Statistics/Fonts/@Count" />
              </td>
              <td class="content">
                <xsl:text>Font</xsl:text>
              </td>
            </tr>
          </table>
        </div>
      </body>
    </html>
  </xsl:template>
</xsl:stylesheet>